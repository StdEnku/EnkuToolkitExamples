<Window x:Class="_12.ViewModelProviderExtension.Views.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:et="https://github.com/StdEnku/EnkuToolkit"
        xmlns:vm="clr-namespace:_12.ViewModelProviderExtension.ViewModels"
        xmlns:local="clr-namespace:_12.ViewModelProviderExtension.Views"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800"
        DataContext="{et:ViewModelProvider vm:MainWindowViewModel}"
        d:DataContext="{d:DesignInstance vm:MainWindowViewModel}">

    <Viewbox>
        <Label Content="{Binding FromViewModelText}" />
    </Viewbox>
</Window>
