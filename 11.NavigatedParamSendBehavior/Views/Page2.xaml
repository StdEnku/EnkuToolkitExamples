<Page x:Class="_11.NavigatedParamSendBehavior.Views.Page2"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:_11.NavigatedParamSendBehavior.Views"
      xmlns:vm="clr-namespace:_11.NavigatedParamSendBehavior.ViewModels"
      d:DataContext="{d:DesignInstance vm:Page2ViewModel}"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="Page2">

    <DockPanel LastChildFill="True">
        <Button Content="Go Back" Margin="10" DockPanel.Dock="Bottom" Command="{Binding ClickedCommand}" />
        <Viewbox DockPanel.Dock="Top">
            <Label Content="{Binding MyText}" />
        </Viewbox>
    </DockPanel>
</Page>
